--8

select count as aantal_gewonnen, count(*) as aantal_spelers
from (select count(wedstrijdnr) as count from  wedstrijden where gewonnen > verloren group by spelersnr) as gewonnen
group by aantal_gewonnen
order by aantal_gewonnen

